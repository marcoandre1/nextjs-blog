{
  "name": "nextjs-blog",
  "version": "1.0.0",
  "description": "Next.js blog app ready to deploy to GitHub Pages using gh-pages",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export && touch ./out/.nojekyll",
    "deploy": "gh-pages -d out -t true",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcoandre1/nextjs-blog.git"
  },
  "keywords": [
    "Next.js",
    "GitHub Pages",
    "gh-pages"
  ],
  "author": "Marco E (https://modokemdev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcoandre1/nextjs-blog/issues"
  },
  "homepage": "https://github.com/marcoandre1/nextjs-blog#readme",
  "dependencies": {
    "date-fns": "^2.16.1",
    "gray-matter": "^4.0.2",
    "next": "^12.0.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "prettier": "2.1.2"
  }
}
